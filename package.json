{
  "name": "react-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest run",
    "e2e": "server-test 'vite dev --port 6789' :6789 'cypress run -q --e2e'",
    "e2e:watch": "server-test 'vite dev --port 6789' :6789 'cypress open --e2e --browser chrome'",
    "lint": "eslint src --ext ts,tsx",
    "lint:fix": "yarn lint --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "@testing-library/jest-dom": "^5.16.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^15.1.3",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.1.5",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/parser": "^5.50.0",
    "@vitejs/plugin-react-swc": "^3.1.0",
    "autoprefixer": "^10.4.13",
    "cypress": "^12.5.1",
    "cypress-mochawesome-reporter": "^3.2.3",
    "eslint": "8.33.0",
    "eslint-config-standard": "17.0.0",
    "eslint-config-standard-jsx": "11.0.0",
    "eslint-config-standard-react": "13.0.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "8.0.3",
    "jsdom": "^21.1.0",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "prettier": "^2.8.3",
    "pretty-quick": "^3.1.3",
    "start-server-and-test": "^1.15.3",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vitest": "^0.28.4"
  },
  "lint-staged": {
    "src/**/*.ts{x}": "yarn lint"
  }
}
